<!DOCTYPE html>
<html ng-app="ryde">
<head>
  <title>Ryde</title>
  <%= render 'layouts/meta' %>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>

<body>
  <%= render 'layouts/header' %>
  <%= render 'layouts/messages' %>
  <%= yield %>
  <%= render 'layouts/footer' %>
</body>
</html>
